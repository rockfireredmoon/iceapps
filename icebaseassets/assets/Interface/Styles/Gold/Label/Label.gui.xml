<?xml version="1.0" encoding="UTF-8"?>
<root>
	<element name="Label">
		<font>
			<property name="fontName" type="String" value="defaultFont" />
			<property name="fontSize" type="float" value="8" />
			<property name="fontColor" type="ColorRGBA">
				<r value="0.8" />
				<g value="0.8" />
				<b value="0.8" />
				<a value="1.0" />
			</property>
			<property name="textAlign" type="String" value="Left" />
			<property name="textVAlign" type="String" value="Center" />
			<property name="textWrap" type="String" value="NoWrap" />
			<property name="textPadding" type="float" value="0" />
		</font>
		<attributes>
			<property name="resizeBorders" type="Vector4f">
				<x value="1" />
				<y value="1" />
				<z value="1" />
				<w value="1" />
			</property>
			<property name="defaultSize" type="Vector2f">
				<x value="100" />
				<y value="30" />
			</property>
		</attributes>
		<images></images>
		<effects></effects>
	</element>
	<element name="ToolTip">
		<font>
			<property name="fontSize" type="float" value="8" />
			<property name="fontName" type="String" value="defaultOutlineFont" />
			<property name="fontColor" type="ColorRGBA">
				<r value="1.0" />
				<g value="1.0" />
				<b value="1.0" />
				<a value="1.0" />
			</property>
			<property name="textAlign" type="String" value="Center" />
			<property name="textVAlign" type="String" value="Center" />
			<property name="textWrap" type="String" value="Word" />
			<property name="textPadding" type="Vector4f">
				<x value="4" />
				<y value="4" />
				<z value="4" />
				<w value="4" />
			</property>
		</font>
		<attributes>
			<property name="resizeBorders" type="Vector4f">
				<x value="1" />
				<y value="1" />
				<z value="1" />
				<w value="1" />
			</property>
			<property name="defaultSize" type="Vector2f">
				<x value="200" />
				<y value="16" />
			</property>
		</attributes>
		<images>
			<property name="defaultImg" type="String" value="Interface/Styles/Gold/Label/tooltip_bg.png" />
		</images>
		<effects></effects>
	</element>
        <element name="LegendLabel">
		<attributes>
			<property name="resizeBorders" type="Vector4f">
				<x value="1" />
				<y value="1" />
				<z value="1" />
				<w value="1" />
			</property>
			<property name="defaultSize" type="Vector2f">
				<x value="16" />
				<y value="16" />
			</property>
		</attributes>
		<images>                    
			<property name="shopImg" type="String" value="Interface/Styles/Gold/Label/vendor.png" />
			<property name="questGiverImg" type="String" value="Interface/Styles/Gold/Label/questgiver.png" />
			<property name="partyMemberImg" type="String" value="Interface/Styles/Gold/Label/party.png" />
                </images>
		<effects></effects>
                <font/>
	</element>
</root>
